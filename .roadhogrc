{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/",
  "theme": {
    "@primary-color": "#1DA57A",
    "@link-color": "#1DA57A",
    "@border-radius-base": "2px",
    "@font-size-base": "16px",
    "@line-height-base": "1.2"
  },
  "autoprefixer": null,
  "proxy": {
    "/api": {
           "target": "http://localhost:3000",
           "changeOrigin": false,
           "pathRewrite": { "^/api" : "" }
         },
   "/list": {
          "target": "http://localhost:3001",
          "changeOrigin": false,
          "pathRewrite": { "^/list" : "" }
        }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
